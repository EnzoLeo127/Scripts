local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/EnzoLeo127/Hub/refs/heads/main/library", true))()
local Flags = Library.Flags

local Window = Library:Window({
    Text = "Russian Game"
})
local Window2 = Library:Window({
    Text = "YT: 14kGreenz"
})

Window:Button({
    Text = "Auto Collect Coins",
    Callback = function()
local tweenService = game:GetService("TweenService")
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local coinsFolder = workspace:FindFirstChild("Coins")
local tweenTime = 1.5
local interval = 3

if not coinsFolder then
    warn("A pasta 'Coins' n√£o foi encontrada na Workspace.")
    return
end

local function tweenToPosition(targetPosition)
    local tweenInfo = TweenInfo.new(
        tweenTime,
        Enum.EasingStyle.Linear,
        Enum.EasingDirection.Out
    )

    local tween = tweenService:Create(humanoidRootPart, tweenInfo, {CFrame = CFrame.new(targetPosition)})
    tween:Play()
    tween.Completed:Wait()
end

for _, coin in pairs(coinsFolder:GetChildren()) do
    if coin:IsA("MeshPart") and coin.Name == "SunCoin" then
        local targetPosition = coin.Position + Vector3.new(0, 3, 0)
        tweenToPosition(targetPosition)
        wait(interval)
    end
end
    end
})

Window2:Button({
    Text = "Copy YouTube link",
    Callback = function()
        setclipboard("https://www.youtube.com/14kGreenz")
    end
})

Window2:Button({
    Text = "Destroy UI",
    Callback = function()
        for _, v in pairs(game:GetService("CoreGui"):GetChildren()) do
            if v.Name == "Revenant" then
                v:Destroy()
            end
        end
    end
})
