local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()




local mainscript = Library:NewWindow("Sour Patch Kids")




local main1 = mainscript:NewSection("Main")




local creditos = mainscript:NewSection("YT: 14kGreenz")




main1:CreateButton("Collect (and TP to Claim Game)", function()

    function Touchinterest()

        spawn(function()

            for _, descendant in ipairs(workspace:GetDescendants()) do

                if descendant:IsA("TouchTransmitter") and

                        game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.PrimaryPart then

                    task.spawn(function()

                        firetouchinterest(descendant:FindFirstAncestorWhichIsA("Part"),

                            game.Players.LocalPlayer.Character.PrimaryPart, 1)

                        task.wait()

                        firetouchinterest(descendant:FindFirstAncestorWhichIsA("Part"),

                            game.Players.LocalPlayer.Character.PrimaryPart, 0)

                    end)

                end

            end

        end)

    end

    Touchinterest()

end)




main1:CreateButton("Claim UGC", function()

    local args = {

        [1] = 1

    }

    game:GetService("ReplicatedStorage").Packages._Index:FindFirstChild("sleitnick_knit@1.4.7").knit.Services.UGCService.RF.TryToClaimAsset:InvokeServer(unpack(args))

end)







creditos:CreateButton("Copy Discord Link", function()

    setclipboard("https://discord.com/invite/nVmWEfj7z4")

end)




creditos:CreateButton("Copy YouTube link", function()

    setclipboard("https://www.youtube.com/14kGreenz")

end)
